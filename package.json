{
  "name": "@raypx/env",
  "version": "0.0.0",
  "description": "Environment validation with extends support and configurable client prefix",
  "type": "module",
  "main": "./dist/index.mjs",
  "types": "./dist/index.d.mts",
  "exports": {
    ".": {
      "import": "./dist/index.mjs",
      "types": "./dist/index.d.mts"
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "tsdown",
    "clean": "rimraf dist",
    "format": "biome format --write .",
    "format:check": "biome format .",
    "lint": "biome lint .",
    "lint:fix": "biome lint --write .",
    "check": "biome check --write .",
    "test": "vitest run",
    "test:watch": "vitest --watch",
    "typecheck": "tsc --noEmit",
    "changeset": "changeset",
    "version": "changeset version",
    "release": "pnpm build && changeset publish && changeset tag && git push --follow-tags",
    "pre:enter": "changeset pre enter",
    "pre:exit": "changeset pre exit",
    "prerelease": "pnpm build && changeset publish",
    "prepublishOnly": "npm run build"
  },
  "keywords": [
    "env",
    "environment-variables",
    "validation",
    "typescript",
    "zod"
  ],
  "author": "Raypx Team",
  "license": "Apache-2.0",
  "repository": {
    "type": "git",
    "url": "https://github.com/raypx/raypx-env.git"
  },
  "dependencies": {
    "envin": "^1.1.10",
    "lodash-es": "^4.17.21",
    "type-fest": "^4.41.0",
    "zod": "^3.25.76"
  },
  "devDependencies": {
    "@biomejs/biome": "^2.3.6",
    "@changesets/cli": "^2.29.7",
    "@types/lodash-es": "^4.17.12",
    "@types/node": "^22.19.1",
    "rimraf": "^6.1.2",
    "tsdown": "^0.9.9",
    "typescript": "^5.9.3",
    "vitest": "^2.1.9"
  },
  "engines": {
    "node": ">=18.0.0"
  }
}
